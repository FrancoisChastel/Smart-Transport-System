<StackPane fx:id="messages" maxHeight="720.0" maxWidth="1280.0" minHeight="720.0" minWidth="1280.0" prefHeight="720.0"
           prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="messagesController">
    <children>
        <Rectangle arcHeight="5.0" fill="#373737" height="721.0" stroke="BLACK" strokeType="INSIDE" width="1280.0"
                   x="-1000.0" StackPane.alignment="TOP_CENTER"/>
        <ListView fx:id="listeMessages" fixedCellSize="122.0" maxHeight="610.0" maxWidth="1290.0" minHeight="610.0"
                  minWidth="1290.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="610.0" prefWidth="1290.0"
                  snapToPixel="false" styleClass="list-cell" StackPane.alignment="BOTTOM_CENTER"/>
        <Rectangle arcHeight="5.0" fill="#1c1c1c" height="110.0" stroke="BLACK" strokeType="INSIDE" width="1280.0"
                   x="-1000.0" StackPane.alignment="TOP_CENTER"/>
        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Messages" StackPane.alignment="TOP_CENTER">
            <font>
                <Font name="Roboto Regular" size="40.0"/>
            </font>
            <StackPane.margin>
                <Insets top="25.0"/>
            </StackPane.margin>
        </Text>
        <Group/>
        <Button id="bouton_Retour" minWidth="97.0" mnemonicParsing="false" onAction="#retourAccueil" prefHeight="75.0"
                prefWidth="75.0" StackPane.alignment="TOP_LEFT">
            <StackPane.margin>
                <Insets left="25.0" top="15.0"/>
            </StackPane.margin>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
    </children>
</StackPane>
