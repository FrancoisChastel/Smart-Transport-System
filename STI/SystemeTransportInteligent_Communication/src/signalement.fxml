                <Insets bottom="380.0" right="800.0"/>
            </StackPane.margin>
        </Text>
        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="ACCIDENT" textAlignment="CENTER"
              wrappingWidth="320.0">
