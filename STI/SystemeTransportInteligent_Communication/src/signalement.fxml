            <StackPane.margin>
                <Insets bottom="380.0" left="800.0"/>
            </StackPane.margin>
        </Text>
        <Button id="bouton_Bouchon" maxHeight="80.0" maxWidth="330.0" minHeight="80.0" minWidth="330.0"
                mnemonicParsing="false" onAction="#signalerBouchon" prefHeight="80.0" prefWidth="330.0" text="SIGNALER"
                textFill="WHITE">
            <StackPane.margin>
                <Insets top="380.0"/>
            </StackPane.margin>
            <font>
                <Font name="Helvetica Neue Light" size="23.0"/>
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button id="bouton_Obstacle" maxHeight="80.0" maxWidth="330.0" minHeight="80.0" minWidth="330.0"
                mnemonicParsing="false" onAction="#signalerPanne" prefHeight="80.0" prefWidth="330.0" text="SIGNALER"
                textFill="WHITE">
            <font>
                <Font name="Helvetica Neue Light" size="23.0"/>
            </font>
            <StackPane.margin>
                <Insets right="800.0" top="380.0"/>
            </StackPane.margin>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button id="bouton_Accident" maxHeight="80.0" maxWidth="330.0" minHeight="80.0" minWidth="330.0"
