        </Rectangle>
        <ImageView fitHeight="230.0" fitWidth="260.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@images/icone_Bouchon.png"/>
            </image>
        </ImageView>
        <ImageView fitHeight="230.0" fitWidth="260.0" pickOnBounds="true" preserveRatio="true">
            <StackPane.margin>
                <Insets right="800.0"/>
            </StackPane.margin>
            <image>
                <Image url="@images/icone_Obstacle.png"/>
            </image>
        </ImageView>
        <ImageView fitHeight="230.0" fitWidth="260.0" pickOnBounds="true" preserveRatio="true">
